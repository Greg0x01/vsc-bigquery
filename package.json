{
    "name": "vsc-bigquery",
    "displayName": "BigQuery",
    "description": "BigQuery Extension",
    "publisher": "christophstockhusen",
    "author": {
        "name": "Christoph Stockhusen",
        "email": "mail@christophstockhusen.de"
    },
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.47.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "icon": "resources/BigQuery.png",
    "license": "MIT",
    "activationEvents": [
        "*",
        "onLanguage:sql",
        "onCommand:extension.dryRun",
        "onCommand:extension.setProjectCommand",
        "onCommand:extension.submit",
        "onCommand:extension.submitRegion",
        "onView:bigQueryResources"
    ],
    "repository": {
        "url": "https://github.com/christophstockhusen/vcs-bigquery"
    },
    "main": "./out/extension.js",
    "contributes": {
        "languages": [
            {
                "id": "BigQuery",
                "aliases": [
                    "BigQuery",
                    "sql"
                ],
                "extensions": [
                    ".sql"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "BigQuery",
                "scopeName": "source.bigquery",
                "path": "./syntaxes/sql.tmLanguage.json"
            }
        ],
        "commands": [
            {
                "category": "BigQuery",
                "title": "Dry Run",
                "command": "extension.dryRun"
            },
            {
                "category": "BigQuery",
                "title": "Run Query",
                "command": "extension.submit",
                "icon": {
                    "light": "resources/light/run.svg",
                    "dark": "resources/dark/run.svg"
                }
            },
            {
                "category": "BigQuery",
                "title": "Submit Region",
                "command": "extension.submitRegion"
            },
            {
                "category": "BigQuery",
                "title": "Set Google Cloud Project",
                "command": "extension.setProjectCommand"
            },
            {
                "category": "BigQuery",
                "command": "bigQueryResources.refreshAllResources",
                "title": "Refresh BigQuery Resources",
                "icon": {
                    "light": "resources/light/refresh.svg",
                    "dark": "resources/dark/refresh.svg"
                }
            },
            {
                "category": "BigQuery",
                "command": "bigQueryResources.showResourceInConsole",
                "title": "Open in Console",
                "icon": {
                    "light": "resources/light/link-external.svg",
                    "dark": "resources/dark/link-external.svg"
                }
            },
            {
                "category": "BigQuery",
                "command": "queryHistory.edit",
                "title": "Edit",
                "icon": {
                    "light": "resources/light/edit.svg",
                    "dark": "resources/dark/edit.svg"
                }
            },
            {
                "category": "BigQuery",
                "command": "queryHistory.showQueryInConsole",
                "title": "Open in Console",
                "icon": {
                    "light": "resources/light/link-external.svg",
                    "dark": "resources/dark/link-external.svg"
                }
            }
        ],
        "keybindings": [
            {
                "command": "extension.dryRun",
                "key": "ctrl+alt+enter",
                "mac": "cmd+alt+enter"
            },
            {
                "when": "editorTextFocus && extension == vsc-bigquery",
                "command": "extension.submit",
                "key": "ctrl+enter",
                "mac": "cmd+enter"
            },
            {
                "command": "extension.submitRegion",
                "key": "ctrl+shift+enter",
                "mac": "cmd+shift+enter"
            }
        ],
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "bigquery",
                    "title": "BigQuery",
                    "icon": "resources/BigQuery.png"
                }
            ]
        },
        "views": {
            "bigquery": [
                {
                    "id": "bigquery.resources",
                    "name": "Resources"
                },
                {
                    "id": "bigquery.queries",
                    "name": "Query History"
                }
            ]
        },
        "menus": {
            "editor/title": [
                {
                    "command": "extension.submit",
                    "when": "resourceLangId == BigQuery",
                    "group": "navigation@-101"
                }
            ],
            "view/title": [
                {
                    "command": "bigQueryResources.refreshAllResources",
                    "when": "view == bigquery",
                    "group": "navigation"
                }
            ],
            "view/item/context": [
                {
                    "command": "bigQueryResources.showResourceInConsole",
                    "when": "view == bigquery.resources && viewItem != field",
                    "group": "inline"
                },
                {
                    "command": "queryHistory.showQueryInConsole",
                    "when": "view == bigquery.queries",
                    "group": "inline"
                },
                {
                    "command": "queryHistory.edit",
                    "when": "view == bigquery.queries",
                    "group": "inline"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "webpack --mode production",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "webpack": "webpack --mode development",
        "webpack-dev": "webpack --mode development --watch",
        "test-compile": "tsc -p ./",
        "postversion": "git push && git push --tags"
    },
    "devDependencies": {
        "@types/node": "^8.10.25",
        "@types/sql-formatter": "^2.3.0",
        "@types/vscode": "^1.47.0",
        "ts-loader": "^8.0.1",
        "tslint": "^5.16.0",
        "typescript": "^3.9.7",
        "vsce": "^1.77.0",
        "webpack": "^4.44.1",
        "webpack-cli": "^3.3.12"
    },
    "dependencies": {
        "@google-cloud/bigquery": "^5.0.1",
        "@google-cloud/resource": "^1.2.1",
        "google-auth-library": "^6.0.5",
        "googleapis": "^55.0.0",
        "sql-formatter": "git+https://github.com/christophstockhusen/sql-formatter.git#bigquery"
    }
}
